<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DropIndicator">
        <attr name="color1" format="color"></attr>
        <attr name="color2" format="color"></attr>
        <attr name="color3" format="color"></attr>
        <attr name="color4" format="color"></attr>
        <attr name="click_color" format="color"></attr>
        <attr name="circle_color" format="color"></attr>
        <attr name="radius" format="dimension"></attr>
        <attr name="scale" format="float"></attr>
        <attr name="duration" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="LineChartReport">
        <attr name="max_score" format="integer"/>
        <attr name="min_score" format="integer"/>
        <attr name="broken_line_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="DottedLine">
        <attr name="lineType" format="integer"/>
        <attr name="lineDotColor" format="color"/>
        <attr name="lineDashGap" format="dimension"/>
        <attr name="lineDashWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SolarEclipse">
        <attr name="elStrokeWidth" format="dimension"/>
        <attr name="elArcColor" format="color"/>
        <attr name="elSunColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ChasingLoading">
        <attr name="clDfmWidth" format="dimension"/>
        <attr name="clDfmSpacing" format="dimension"/>
        <attr name="clDsRate" format="integer"/>
        <attr name="clFsRate" format="integer"/>
        <attr name="clMsRate" format="integer"/>
        <attr name="clDsAngle" format="integer"/>
        <attr name="clFsAngle" format="integer"/>
        <attr name="clMsAngle" format="integer"/>
        <attr name="clChasing" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CompassServant">
        <attr name="cs_color_commander" format="integer"/>
        <attr name="cs_color1" format="color"/>
        <attr name="cs_color2" format="color"/>
        <attr name="cs_color3" format="color"/>
        <attr name="cs_color4" format="color"/>
    </declare-styleable>
</resources>